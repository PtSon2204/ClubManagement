<Window x:Class="ClubManagementUI.HomePage.HomeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClubManagementUI.HomePage"
        mc:Ignorable="d"
        Title="HomeMemberWindow" Height="600" Width="1200"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <TextBlock x:Name="WelcomeText" FontSize="20" 
                   HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,20,0,0"/>

        <Button Content="Logout" Width="100" Height="30" 
                HorizontalAlignment="Right" VerticalAlignment="Top" 
                Margin="0,20,20,0"  Style="{StaticResource LogoutButtonStyle}"  Click="Logout_Click"/>
        
        
    </Grid>
</Window>
